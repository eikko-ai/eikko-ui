{
  "name": "@kai/components",
  "private": true,
  "version": "0.1.0",
  "description": "Kai UI component library",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "babelMacros": {
    "twin": {
      "config": "./tailwind.config.js",
      "preset": "styled-components"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:ts": "tsc",
    "build:js": "BUILD=production rollup -c",
    "build": "yarn clean && yarn build:ts && yarn build:js",
    "watch:ts": "tsc --watch",
    "watch:js": "rollup -c --watch",
    "watch": "yarn clean && yarn watch:ts & yarn watch:js",
    "storybook": "start-storybook -p 6060",
    "storybook:export": "build-storybook",
    "lint": "eslint . --ignore-path ./.gitignore",
    "lint:fix": "eslint . --fix --ignore-path ./.gitignore",
    "format": "prettier --write \"**/*.{js,jsx,ts, tsx,json,md}\"",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "ui",
    "eikko",
    "components",
    "tailwindcss",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eikko-ai/kai.git"
  },
  "bugs": {
    "url": "https://github.com/eikko-ai/kai/issues"
  },
  "author": "Nuno Freire <nuno.freire@eikko.ai>",
  "homepage": "https://github.com/eikko-ai/kai#readme",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "postcss": "^8.2.6"
  },
  "dependencies": {
    "@react-aria/utils": "^3.6.0",
    "@tailwindcss/forms": "^0.2.1",
    "@types/tinycolor2": "^1.4.2",
    "memoize-one": "^5.1.1",
    "react-hot-toast": "^1.0.2",
    "styled-components": "^5.2.1",
    "tailwindcss": "^2.0.3",
    "tinycolor2": "^1.4.2",
    "twin.macro": "^2.2.3"
  }
}
