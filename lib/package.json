{
  "name": "@eikko-ui/lib",
  "version": "0.0.1",
  "description": "React component library by eikko.ai",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/cjs/*",
    "dist/esm/*"
  ],
  "scripts": {
    "clean": "rimraf dist .next",
    "build": "npm run build:ts && BUILD=production rollup -c",
    "build:ts": "tsc --noEmit false --declaration true --outDir dist/esm --declarationDir dist/esm",
    "build:tailwind": "tailwindcss build lib/styles/tailwind.pcss -o lib/styles/main.css -c tailwind.config.js",
    "build:watch": "tsc -w --declaration true --outDir dist/esm --declarationDir dist/esm && BUILD=development rollup -cw",
    "build:docs": "next build",
    "__prepare": "npm run clean && npm run build",
    "lint": "eslint ."
  },
  "author": "Nuno Freire",
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "^2.0.1"
  },
  "dependencies": {
    "autoprefixer": "^10.0.2",
    "postcss": "^8.1.7"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/node": "^14.14.9",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.33.3",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.1.2"
  }
}
