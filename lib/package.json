{
  "name": "@eikko-ui/lib",
  "version": "0.0.1",
  "description": "React component library by eikko.ai",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "README.me"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run tailwind:build && npm run build:ts ",
    "build": "BUILD=production rollup -c",
    "build:ts": "tsc --noEmit false --declaration true --outDir dist/esm --declarationDir dist/esm",
    "build:watch": "npm run tailwind:dev && tsc -w --declaration true --outDir dist/esm --declarationDir dist/esm & BUILD=development rollup -c -w",
    "tailwind:dev": "tailwindcss build src/styles/tailwind.pcss -o src/styles/main.css -c tailwind.config.js",
    "tailwind:build": "cross-env NODE_ENV=production tailwindcss build src/styles/tailwind.pcss -o src/styles/main.css -c tailwind.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "_prepare": "npm run clean && npm run build"
  },
  "author": "Nuno Freire",
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "^2.0.1"
  },
  "dependencies": {
    "autoprefixer": "^10.0.3",
    "postcss": "^8.1.10"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "cross-env": "^7.0.2",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.3",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-styles": "^3.11.0",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.1.2"
  }
}
