{
  "name": "kai",
  "private": true,
  "version": "1.0.0",
  "description": "Kai UI component library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.js",
      "preset": "styled-components"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "build:ts": "tsc",
    "build:js": "BUILD=production rollup -c",
    "build": "npm run build:ts && npm run build:js",
    "watch:ts": "tsc --watch",
    "watch:js": "rollup -c --watch",
    "watch": "npm run watch:ts & npm run watch:js",
    "dev": "npm run watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "eikko",
    "ui",
    "components",
    "react",
    "preact",
    "tailwindcss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eikko-ai/kai.git"
  },
  "bugs": {
    "url": "https://github.com/eikko-ai/kai/issues"
  },
  "homepage": "https://github.com/eikko-ai/kai#readme",
  "author": "Nuno Freire <nuno.freire@eikko.ai>",
  "license": "MIT",
  "browserslist": "> 0.25%, not dead",
  "peerDependencies": {
    "preact": "^10.5.7",
    "styled-components": "^5.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "babel-plugin-macros": "^3.0.1",
    "core-js": "^3.8.1",
    "postcss": "^8.2.1",
    "tailwindcss": "^2.0.2",
    "twin.macro": "^2.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.11",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@types/styled-components": "^5.1.7",
    "preact": "^10.5.7",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.3"
  }
}
